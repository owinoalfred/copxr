    "jest": "26.0.1",
    "mini-css-extract-plugin": "0.9.0",
    "parallel-webpack": "2.6.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "serve-handler": "6.1.2",
    "tailwindcss": "1.4.6",
    "vue-jest": "3.0.5",
