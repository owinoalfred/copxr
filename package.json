{
  "name": "xr-global-vue",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start:dev": "PORT=4000 browser-refresh renderer/dev/server.js",
    "start": "cross-env NODE_ENV=production node ./renderer/generate/index.js",
    "start:feed": "cross-env NODE_ENV=production node ./renderer/generate/feed.js",
    "build:dev": "webpack --progress --watch",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=production parallel-webpack",
    "clean": "node renderer/generate/clean.js",
    "copy:assets": "rm -f public/assets/img/events/full/*; mkdir -p public/assets/img/events/full; cp -f _data/events/images/* public/assets/img/events/full/",
    "generate": "npm run clean && npm run copy:assets && npm run build && npm run start",
    "eslint": "eslint --ext .js,.json,.vue .",
    "unit": "jest",
    "unit:ci": "jest --ci --collectCoverageFrom='./{src,renderer}/**/*.{js,vue}'",
    "unit:dev": "jest --watch",
    "test": "npm run eslint && npm run unit"
  },
  "engines": {
    "node": "12.16.1",
    "npm": "6.13.4"
  },
  "dependencies": {
    "@tailwindcss/typography": "0.2.0",
    "@tailwindcss/ui": "0.3.0",
    "axios": "0.19.2",
    "browser-refresh": "1.7.3",
    "core-js": "3.6.5",
    "date-fns": "2.14.0",
    "feed": "4.1.0",
    "gray-matter": "4.0.2",
    "intersection-observer": "0.10.0",
    "leaflet": "1.6.0",
    "leaflet-fullscreen": "1.0.2",
    "leaflet.markercluster": "1.4.1",
    "markdown-it": "11.0.0",
