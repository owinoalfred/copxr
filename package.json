    "markdown-it-footnote": "3.0.2",
    "vue": "2.6.11",
    "vue-clickaway": "2.2.2",
    "vue-i18n": "8.17.7",
    "vue-material-design-icons": "4.6.0",
    "vue-server-renderer": "2.6.11",
    "vue-template-compiler": "2.6.11"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/register": "7.9.0",
    "@types/jest": "25.2.3",
